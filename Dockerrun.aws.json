{
    "AWSEBDockerrunVersion": 1,
    "containerDefinitions":[
        {
            "name": "api",
            "image": "jessemellinger/apiv2",
            "hostname": "api",
            "essential": true,
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 5000
                }
            ],
            "command": [
                "tail",
                "-f",
                "/bin/bash"
            ]
        }
    ]
}